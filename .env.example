# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Choose LLM Provider: openai or google
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o

# Database Configuration (Supabase or PostgreSQL)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Or use PostgreSQL directly
# DATABASE_URL=postgresql://user:password@localhost:5432/talent_hunter

# Gmail API
GMAIL_CREDENTIALS_PATH=credentials/gmail_credentials.json
GMAIL_TOKEN_PATH=credentials/gmail_token.json

# Google Calendar API
CALENDAR_CREDENTIALS_PATH=credentials/calendar_credentials.json
CALENDAR_TOKEN_PATH=credentials/calendar_token.json

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
